{
  "expo": {
    "name": "SolarPanelMeasurementApp",
    "slug": "solarpanelmeasurementapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.tvoje.jmeno.solarapp",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryAddUsageDescription": "App needs access to save photos to your gallery."
      }
    },
    "android": {
      "package": "com.tvoje.jmeno.solarapp",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Používáme vaši galerii pro ukládání fotek měření."
        }
      ],
      "./with-text-recognition"
    ],
    "extra": {
      "eas": {
        "projectId": "6070459b-138b-499e-a0ce-aecd639818e0"
      }
    },
    "owner": "akrij23"
  }
}